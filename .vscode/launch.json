{
    
        // Use IntelliSense to learn about possible attributes.
        // Hover to view descriptions of existing attributes.
        // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [

            
            {
                "type": "java",
                "name": "ShopeeList",
                "request": "launch",
                "mainClass": "movie.core/core.ShopeeList",
                "projectName": "modules-core"
            },
            {
                "type": "java",
                "name": "App",
                "request": "launch",
                "mainClass": "shopee.ui/ui.shopee.Shopee",
                "projectName": "modules-ui"
            },
            {
                "type": "java",
                "name": "Current File",
                "request": "launch",
                "mainClass": "${file}"
            },
            {
                "type": "java",
                "name": "Launch modules-template - multi-module, multi-package ",
                "request": "launch",
                "mainClass": "shopee.ui/shopee.ui",
                "projectName": "modules-ui",
                "vmArgs": [
                    "--add-opens",
                    "shopee.ui/shopee.ui=ALL-UNNAMED",
                    "--add-exports",
                    "javafx.graphics/com.sun.javafx.application=ALL-UNNAMED"
                ]
            }
        ]
    
}